navToggle.addEventListener("click", () => {
  secondaryNavigation.hasAttribute("data-visible") ?
  navToggle.setAttribute("aria-expanded", false): 
  navToggle.setAttribute ("aria-expanded", true );
  secondaryNavigation.toggleAttribute("data-visible"); 
  secondaryNavigation.toggleAttribute("data-overlay");

  navToggle.addEventListener("click", () => {
    const isNavVisible = secondaryNavigation.getAttribute("data-visible") === "true";
    navToggle.setAttribute("aria-expanded", !isNavVisible);
    secondaryNavigation.setAttribute("data-visible", !isNavVisible);
    secondaryNavigation.toggleAttribute("data-overlay", !isNavVisible);
  
    // Switch between hamburger and close icons
    if (openIcon && closeIcon) {
        openIcon.classList.toggle('hidden');
        closeIcon.classList.toggle('hidden');
    }