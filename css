const navToggle = document.querySelector(".mobile-nav-toggle");
const secondaryNavigation = document.querySelector(".secondary-navigation");

navToggle.addEventListener("click", () => {
  secondaryNavigation.hasAttribute("data-visible") ?
  navToggle.setAttribute("aria-expanded", false): 
  navToggle.setAttribute ("aria-expanded", true );
  secondaryNavigation.toggleAttribute("data-visible"); 
  secondaryNavigation.toggleAttribute("data-overlay");
});

.secondary-navigation{
  display: none;
}
.secondary-navigation.show{
  display: block;

}

